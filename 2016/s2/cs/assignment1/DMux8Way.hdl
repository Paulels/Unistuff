/**
 * 8-way demultiplexor:
 * {a, b, c, d, e, f, g, h} = {in, 0, 0, 0, 0, 0, 0, 0} if sel == 000
 *                            {0, in, 0, 0, 0, 0, 0, 0} if sel == 001
 *                            etc.
 *                            {0, 0, 0, 0, 0, 0, 0, in} if sel == 111
 */

CHIP DMux8Way {
    IN in, sel[3];
    OUT a, b, c, d, e, f, g, h;

    PARTS:
    Dmux(in=in,sel=sel[2],a=dabcd,b=defgh);		//outing the input in the right path again

    Dmux4Way(in=dabcd,sel[0]=sel[0],sel[1]=sel[1],a=a,b=b,c=c,d=d);		//will channel in towards the right output
    Dmux4Way(in=defgh,sel[0]=sel[0],sel[1]=sel[1],a=e,b=f,c=g,d=h);
}
